{
  "name": "resume-pdf-generator-cli",
  "version": "1.0.0",
  "description": "A utility to turn a JSON file with personal resume info into a nicely-formatted PDF document using a JSX template",
  "main": "index.tsx",
  "scripts": {
    "build": "npm run build:macos-arm64 && npm run build:windows",
    "build:native": "bun build --compile --outfile dist/resume-generate index.tsx",
    "build:macos-arm64": "bun build --compile --target=bun-darwin-arm64 --outfile dist/resume-generate-arm64 index.tsx",
    "build:macos-x64": "bun build --compile --target=bun-darwin-x64 --outfile dist/resume-generate-x64 index.tsx",
    "build:windows": "bun build --compile --target=bun-windows-x64  --outfile dist/resume_generate.exe index.tsx",
    "start": "bun run index.tsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "PDF",
    "JSON",
    "React",
    "JSX"
  ],
  "author": "Alex Staples",
  "license": "MIT",
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@types/react": "^19.1.8",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "bun": "^1.2.19",
    "typescript": "^5.8.3"
  },
  "type": "module"
}
